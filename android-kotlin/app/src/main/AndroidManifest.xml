<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ibm.appconfiguration.example.bluecharge">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".MobileActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".LoginRegisterActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".FlightActivity" />
        <activity android:name=".DTHActivity" />
        <activity android:name=".BusActivity" />
        <activity android:name=".BroadbandActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>